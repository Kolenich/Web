{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "rules": {
    "indent": false,
    "no-unused-variable": true,
    "no-for-in-array": true,
    "import-name": [
      false,
      {
        "app": "App",
        "config": "getConfig",
        "document": "Document",
        "error": "Error",
        "head": "Head",
        "link": "Link",
        "router": "Router"
      }
    ],
    "no-trailing-whitespace": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ]
  }
}
