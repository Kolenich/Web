{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "rules": {
    "member-ordering": true,
    "promise-function-async": true,
    "curly": true,
    "no-construct": true,
    "no-var-keyword": true,
    "prefer-object-spread": true,
    "completed-docs": true,
    "interface-name": true,
    "ordered-imports": true,
    "jsdoc-format": true,
    "indent": false,
    "no-unused-variable": true,
    "no-for-in-array": true,
    "import-name": [
      false,
      {
        "app": "App",
        "config": "getConfig",
        "document": "Document",
        "error": "Error",
        "head": "Head",
        "link": "Link",
        "router": "Router"
      }
    ],
    "no-trailing-whitespace": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-snake-case"
    ]
  }
}
